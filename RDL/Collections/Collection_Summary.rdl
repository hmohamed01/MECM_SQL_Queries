        </Field>
        <Field Name="Incremental_Updates">
          <DataField>Incremental Updates</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Limiting_Collection">
          <DataField>Limiting Collection</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
