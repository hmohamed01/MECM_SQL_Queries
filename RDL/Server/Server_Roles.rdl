        </Field>
        <Field Name="Role_ID">
          <DataField>Role ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Install_State">
          <DataField>Install State</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
