  <DataSources>
    <DataSource Name="CMDatabase">
      <DataSourceReference>CMDatabase</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
    </DataSource>
  </DataSources>
  <ReportParameters>
    <ReportParameter Name="InactiveDays">
      <DataType>Integer</DataType>
      <Prompt>InactiveDays</Prompt>
      <DefaultValue>
        <Values>
          <Value>30</Value>
        </Values>
      </DefaultValue>
    </ReportParameter>
  </ReportParameters>
  <DataSets>
    <DataSet Name="MainQuery">
      <Query>
        <DataSourceName>CMDatabase</DataSourceName>
        <CommandText>SELECT
    sys.ResourceID,
    sys.Name0 AS [Computer Name],
    sys.UserName0 AS [User Name],
    sys.AD_Site_Name0 AS [AD Site],
    COALESCE(FORMAT(ch.LastDDR, 'yyyy-MM-dd HH:mm'), 'Never') AS [Last DDR],
    COALESCE(FORMAT(ch.LastHW, 'yyyy-MM-dd HH:mm'), 'Never') AS [Last HW Scan],
    COALESCE(FORMAT(ch.LastPolicyRequest, 'yyyy-MM-dd HH:mm'), 'Never') AS [Last Policy Request],
    DATEDIFF(DAY, ch.LastDDR, GETDATE()) AS [Days Inactive],
    CASE
        WHEN DATEDIFF(DAY, ch.LastDDR, GETDATE()) &gt; 90 THEN 'Critical (&gt; 90 days)'
        WHEN DATEDIFF(DAY, ch.LastDDR, GETDATE()) &gt; 60 THEN 'Warning (&gt; 60 days)'
        ELSE 'Inactive (&gt; 30 days)'
    END AS [Status]
FROM v_R_System_Valid sys
LEFT JOIN v_CH_ClientSummary ch
    ON sys.ResourceID = ch.ResourceID
WHERE ch.LastDDR IS NULL
    OR DATEDIFF(DAY, ch.LastDDR, GETDATE()) &gt; @InactiveDays
ORDER BY ch.LastDDR ASC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@InactiveDays">
            <Value>=Parameters!InactiveDays.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
